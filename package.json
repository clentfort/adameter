		"@types/node": "^24.0.0",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@vitejs/plugin-react": "^5.0.0",
		"@vitest/coverage-v8": "4.0.18",
		"concurrently": "^9.2.0",
		"eslint": "^10.0.0",
