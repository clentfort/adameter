{
	"name": "adameter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"predev": "pnpm run fbtee:collect && pnpm run fbtee:translate",
		"dev:next": "next dev",
		"dev:partykit": "partykit dev",
		"dev": "concurrently \"pnpm:dev:*\"",
		"prebuild": "pnpm run fbtee:collect && pnpm run fbtee:translate",
		"build": "next build",
		"start": "next start",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "vitest",
		"fbtee:collect": "fbtee collect --common ./common_strings.json --src src",
		"fbtee:translate": "fbtee translate --translations translations/*.json -o src/translations",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{cjs,js,mjs,jsx,ts,tsx}": [
			"pnpm exec eslint --fix --max-warnings=0",
			"pnpm exec prettier --write"
		],
		"*.{css,json,md,yaml,yml}": "pnpm exec prettier --write"
	},
	"devEngines": {
		"packageManager": {
			"name": "pnpm",
			"onFail": "error",
			"version": ">=9"
		}
	},
	"dependencies": {
		"@base-ui/react": "^1.2.0",
		"@hookform/resolvers": "^5.1.1",
		"@nkzw/core": "^1.2.0",
		"@pedi-growth/core": "^1.1.0",
		"@tailwindcss/postcss": "^4.1.4",
		"@types/file-saver": "^2.0.7",
		"@types/friendly-words": "^1.2.2",
		"@types/papaparse": "^5.5.2",
		"@types/qrcode.react": "^3.0.0",
		"chart.js": "latest",
		"chartjs-adapter-date-fns": "latest",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"date-fns": "latest",
		"fast-deep-equal": "^3.1.3",
		"fbtee": "1.7.2",
		"file-saver": "^2.0.5",
		"friendly-words": "^1.3.1",
		"html5-qrcode": "^2.3.8",
		"jszip": "^3.10.1",
		"lucide-react": "^0.574.0",
		"next": "16.1.6",
		"next-themes": "^0.4.6",
		"papaparse": "^5.5.3",
		"partysocket": "1.1.13",
		"qrcode.react": "^4.2.0",
		"react": "^19",
		"react-confetti": "^6.4.0",
		"react-dom": "^19",
		"react-hook-form": "^7.59.0",
		"react-markdown": "^10.1.0",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.2.0",
		"tailwindcss": "^4.1.4",
		"tinybase": "^7.3.3",
		"tw-animate-css": "^1.2.8",
		"y-indexeddb": "^9.0.12",
		"y-partykit": "^0.0.33",
		"yjs": "^13.6.27",
		"zod": "^4.0.0"
	},
	"devDependencies": {
		"@babel/runtime": "^7.27.0",
		"@eslint/eslintrc": "^3.3.3",
		"@ianvs/prettier-plugin-sort-imports": "^4.4.1",
		"@next/eslint-plugin-next": "^16.1.6",
		"@nkzw/babel-preset-fbtee": "^1.7.0",
		"@nkzw/eslint-config": "^3.3.0",
		"@nkzw/eslint-plugin-fbtee": "^1.7.1",
		"@playwright/test": "^1.58.2",
